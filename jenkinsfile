pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {

                git branch: 'master', url: 'https://github.com/attia12/Pipeline.git'
            }
        }
        stage('Compile') {
            steps {

                sh './mvnw clean compile'
            }
        }
        stage('Test') {
            steps {

                sh './mvnw test'
            }
        }
    }

    post {
        always {

            junit 'target/surefire-reports/*.xml'


            cleanWs()
        }
    }
}